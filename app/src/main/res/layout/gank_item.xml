<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="result"
            type="com.sinyuk.yukdaily.entity.Gank.GankResult"/>
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ViewStub
            android:id="@+id/androidStub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout="@layout/gank_item_layout"
            app:dataset="@{result.anzhuo}"/>

        <ViewStub
            android:id="@+id/iosStub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout="@layout/gank_item_layout"
            app:dataset="@{result.ios}"/>


        <ViewStub
            android:id="@+id/frontEndStub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout="@layout/gank_item_layout"
            app:dataset="@{result.frontEnd}"/>

        <ViewStub
            android:id="@+id/plusStub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout="@layout/gank_item_layout"
            app:dataset="@{result.plus}"/>
    </LinearLayout>

</layout>